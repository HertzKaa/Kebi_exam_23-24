@prefix : <http://www.semanticweb.org/fabio/ontologies/2025/5/untitled-ontology-60#> .
@prefix co: <http://www.co-ode.org/ontologies/ont.owl#> .
@prefix pm: <http://example.org/personalized-menu#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix recomm: <http://www.semanticweb.org/fabio/ontologies/2025/5/PersonalizedMenu#> .
@base <http://www.semanticweb.org/fabio/ontologies/2025/5/untitled-ontology-60#> .

<http://www.semanticweb.org/fabio/ontologies/2025/5/untitled-ontology-60> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/fabio/ontologies/2025/5/PerzonalizedMenu#isCategoryOf
<http://www.semanticweb.org/fabio/ontologies/2025/5/PerzonalizedMenu#isCategoryOf> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/personalized-menu#A_IsContainedIn
pm:A_IsContainedIn rdf:type owl:ObjectProperty ;
                   owl:inverseOf pm:ContainsAllergen ;
                   rdfs:domain pm:Allergen ;
                   rdfs:range pm:Meal .


###  http://example.org/personalized-menu#ContainsAllergen
pm:ContainsAllergen rdf:type owl:ObjectProperty ;
                    rdfs:domain pm:Meal ;
                    rdfs:range pm:Allergen .


###  http://example.org/personalized-menu#HasIngredient
pm:HasIngredient rdf:type owl:ObjectProperty ;
                 owl:inverseOf pm:I_IsContainedIn ;
                 rdfs:domain pm:Meal ;
                 rdfs:range pm:Ingredients .


###  http://example.org/personalized-menu#I_IsContainedIn
pm:I_IsContainedIn rdf:type owl:ObjectProperty ;
                   rdfs:domain pm:Ingredients ;
                   rdfs:range pm:Meal .


###  http://www.co-ode.org/ontologies/ont.owl#hasAllergy
co:hasAllergy rdf:type owl:ObjectProperty ;
              rdfs:domain co:Guest ;
              rdfs:range pm:Allergen .


###  http://www.co-ode.org/ontologies/ont.owl#hasCaloriePreference
co:hasCaloriePreference rdf:type owl:ObjectProperty ;
                        rdfs:domain co:Guest ;
                        rdfs:range co:CaloriePreference .


###  http://www.co-ode.org/ontologies/ont.owl#hasCategory
co:hasCategory rdf:type owl:ObjectProperty ;
               rdfs:domain pm:Meal ;
               rdfs:range co:Categories .


###  http://www.co-ode.org/ontologies/ont.owl#hasDietType
co:hasDietType rdf:type owl:ObjectProperty ;
               rdfs:domain co:Guest ;
               rdfs:range co:DietType .


###  http://www.semanticweb.org/fabio/ontologies/2025/5/PersonalizedMenu#notRecommendedFor
recomm:notRecommendedFor rdf:type owl:ObjectProperty ;
                         rdfs:domain pm:Meal ;
                         rdfs:range co:Guest .


#################################################################
#    Data properties
#################################################################

###  http://example.org/personalized-menu#Calories
pm:Calories rdf:type owl:DatatypeProperty ;
            rdfs:domain pm:Meal ;
            rdfs:range xsd:integer .


###  http://example.org/personalized-menu#IngredientName
pm:IngredientName rdf:type owl:DatatypeProperty ;
                  rdfs:domain pm:Ingredients ;
                  rdfs:range xsd:string .


###  http://example.org/personalized-menu#MealName
pm:MealName rdf:type owl:DatatypeProperty ;
            rdfs:domain pm:Meal ;
            rdfs:range xsd:string .


###  http://example.org/personalized-menu#isHeavyMeal
pm:isHeavyMeal rdf:type owl:DatatypeProperty ;
               rdfs:domain pm:Meal ;
               rdfs:range xsd:boolean .


###  http://example.org/personalized-menu#isLightMeal
pm:isLightMeal rdf:type owl:DatatypeProperty ;
               rdfs:domain pm:Meal ;
               rdfs:range xsd:boolean .


###  http://www.co-ode.org/ontologies/ont.owl#GuestName
co:GuestName rdf:type owl:DatatypeProperty ;
             rdfs:domain co:Guest ;
             rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://example.org/personalized-menu#Allergen
pm:Allergen rdf:type owl:Class .


###  http://example.org/personalized-menu#AnimalBasedIngredient
pm:AnimalBasedIngredient rdf:type owl:Class ;
                         rdfs:subClassOf pm:Ingredients .


###  http://example.org/personalized-menu#Ingredients
pm:Ingredients rdf:type owl:Class .


###  http://example.org/personalized-menu#Meal
pm:Meal rdf:type owl:Class .


###  http://www.co-ode.org/ontologies/ont.owl#CaloriePreference
co:CaloriePreference rdf:type owl:Class .


###  http://www.co-ode.org/ontologies/ont.owl#Categories
co:Categories rdf:type owl:Class .


###  http://www.co-ode.org/ontologies/ont.owl#DietType
co:DietType rdf:type owl:Class .


###  http://www.co-ode.org/ontologies/ont.owl#Guest
co:Guest rdf:type owl:Class .


###  http://www.semanticweb.org/fabio/ontologies/2025/5/untitled-ontology-41#VeganForbiddenIngredient
<http://www.semanticweb.org/fabio/ontologies/2025/5/untitled-ontology-41#VeganForbiddenIngredient> rdf:type owl:Class ;
                                                                                                   rdfs:subClassOf pm:Ingredients .


